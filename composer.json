{
    "name": "nextqs/qrcode-library",
    "description": "QrCode Generator",
    "type": "library",
    "keywords": [
        "2amigos",
        "nextqs",
        "extension",
        "component",
        "qrcode",
        "qr"
    ],
    "homepage": "https://2amigos.us/open-source/details/qrcode-library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "nextqs",
            "email": "nextqs@nextqs.com",
            "homepage": "https://nextqs.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nextqs/qrcode-library/issues",
        "source": "https://github.com/nextqs/qrcode-library"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-gd": "*",
        "ext-simplexml": "*",
        "nextqs/bacon-qr-code": "1.0.4.x-dev",
        "khanamiryan/qrcode-detector-decoder": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.3",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "@stable",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1",
        "codeception/codeception": "*",
        "yiisoft/yii2": "^2.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-asserts": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Da\\QrCode\\": "./src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
